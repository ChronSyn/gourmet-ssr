{
  "name": "@gourmet/test-multi-views",
  "version": "0.3.2",
  "private": true,
  "scripts": {
    "start": "node lib/server.js",
    "build": "gourmet build",
    "test": "npm run test:local && npm run test:prod",
    "test:local": "gourmet build --verbose 1 && tape test/*_test.js",
    "test:prod": "gourmet build --verbose 1 --stage prod && tape test/*_test.js --stage prod"
  },
  "dependencies": {
    "@gourmet/client-lib": "^0.6.2",
    "@gourmet/server-args": "^0.4.0",
    "express": "^4.16.3",
    "morgan": "^1.9.0"
  },
  "devDependencies": {
    "@gourmet/gourmet-cli": "^0.5.0",
    "@gourmet/preset-react": "^0.6.2",
    "@gourmet/preset-react-i80": "^0.3.0",
    "@gourmet/promise-tape": "^0.3.0",
    "@gourmet/puppeteer-args": "^0.4.0",
    "got": "^8.3.1",
    "puppeteer": "^1.3.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "tape": "^4.9.0"
  }
}
