{
  "name": "@gourmet/test-builder-deps",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "build": "gourmet build",
    "watch": "node lib/server.js --watch",
    "test": "yarn test:local && yarn test:prod",
    "test:local": "gourmet build --verbose 1 --ignore-compile-errors && tape test/*_test.js",
    "test:prod": "gourmet build --verbose 1 --stage prod --ignore-compile-errors && tape test/*_test.js --stage prod"
  },
  "dependencies": {
    "@gourmet/client-lib": "^0.6.2",
    "@gourmet/server-args": "^0.4.0",
    "express": "^4.16.3",
    "morgan": "^1.9.0",
    "classnames": "^2.2.6",
    "mkdirp": "^0.5.1"
  },
  "devDependencies": {
    "@gourmet/gourmet-cli": "^0.5.0",
    "@gourmet/preset-core": "^0.5.2",
    "@gourmet/promise-tape": "^0.3.0",
    "@gourmet/puppeteer-args": "^0.4.0",
    "domready": "^1.0.8",
    "puppeteer": "^1.3.0",
    "tape": "^4.9.0"
  }
}
