{
  "name": "@gourmet/test-postcss",
  "version": "0.2.1",
  "private": true,
  "scripts": {
    "build": "gourmet build",
    "build:local": "gourmet build --verbose 1",
    "build:prod": "gourmet build --verbose 1 --stage prod",
    "build:file": "gourmet build --verbose 1 --stage file",
    "build:config": "gourmet build --verbose 1 --stage config",
    "test": "npm run build:local && npm run build:prod && npm run build:file && npm run build:config && tape test/*_test.js"
  },
  "dependencies": {
    "@gourmet/client-lib": "^0.8.0",
    "@gourmet/server-args": "^0.6.0",
    "express": "^4.16.3",
    "morgan": "^1.9.0"
  },
  "devDependencies": {
    "@gourmet/gourmet-cli": "^0.7.0",
    "@gourmet/preset-core": "^0.10.1",
    "postcss-color-rgba-fallback": "^3.0.0",
    "tape": "^4.9.0"
  }
}
