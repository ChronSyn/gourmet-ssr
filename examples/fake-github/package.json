{
  "name": "@gourmet/example-fake-github",
  "version": "0.5.1",
  "private": true,
  "scripts": {
    "hot": "node server/lib/index.js --hot",
    "watch": "node server/lib/index.js --watch",
    "start": "node server/lib/index.js",
    "build": "gourmet build",
    "test": "yarn test:local && yarn test:prod",
    "test:local": "gourmet build --verbose 0 && tape test/*_test.js",
    "test:prod": "gourmet build --verbose 0 --stage prod && tape test/*_test.js --stage prod"
  },
  "dependencies": {
    "@gourmet/server-impl-watch": "^0.4.0"
  },
  "devDependencies": {
    "@gourmet/gourmet-cli": "^0.3.0",
    "@gourmet/preset-react-emotion": "^0.4.1",
    "@gourmet/promise-tape": "^0.3.0",
    "@gourmet/puppeteer-args": "^0.3.0",
    "@gourmet/server-args": "^0.2.0",
    "bootstrap": "^4.1.0",
    "classnames": "^2.2.6",
    "ionicons": "^2.0.1",
    "puppeteer": "^1.3.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-prepare": "asyncmax/react-prepare",
    "reactstrap": "^5.0.0",
    "tape": "^4.9.0",
    "tinycolor2": "^1.4.1"
  }
}
