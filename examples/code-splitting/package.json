{
  "name": "@gourmet/example-code-splitting",
  "version": "0.4.3",
  "private": true,
  "scripts": {
    "hot": "node lib/devel.js --hot",
    "watch": "node lib/devel.js --watch",
    "start": "node lib/devel.js",
    "build": "gourmet build",
    "test": "yarn test:local && yarn test:prod",
    "test:local": "gourmet build --verbose 1 && tape test/*_test.js",
    "test:prod": "gourmet build --verbose 1 --stage prod && tape test/*_test.js --stage prod"
  },
  "dependencies": {
    "@gourmet/server-impl-watch": "^0.4.0"
  },
  "devDependencies": {
    "@gourmet/gourmet-cli": "^0.3.0",
    "@gourmet/preset-react-emotion": "^0.4.3",
    "@gourmet/promise-tape": "^0.3.0",
    "@gourmet/puppeteer-args": "^0.3.0",
    "@gourmet/server-args": "^0.2.0",
    "classnames": "^2.2.6",
    "got": "^8.3.1",
    "puppeteer": "^1.3.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "tape": "^4.9.0"
  }
}
