{
  "name": "@gourmet/example-code-splitting",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "hot": "node server/lib/devel.js --hot",
    "watch": "node server/lib/devel.js --watch",
    "test": "yarn test:local && yarn test:prod",
    "test:local": "gourmet build --verbose 0 && tape test/*_test.js",
    "test:prod": "gourmet build --verbose 0 --stage prod && tape test/*_test.js --stage prod"
  },
  "dependencies": {
    "@gourmet/server-impl-watch": "^0.2.4"
  },
  "devDependencies": {
    "@gourmet/gourmet-cli": "^0.2.0",
    "@gourmet/preset-builder": "^0.2.5",
    "@gourmet/preset-react-emotion": "^0.2.2",
    "@gourmet/promise-tape": "^0.2.0",
    "@gourmet/puppeteer-args": "^0.2.0",
    "@gourmet/server-args": "^0.1.1",
    "got": "^8.3.1",
    "puppeteer": "^1.3.0",
    "tape": "^4.9.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2"
  }
}
